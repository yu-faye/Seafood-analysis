      "type": "LinkedServiceReference"
    },
    "schema": [
      {
        "name": "event_id",
        "type": "String"
      },
      {
        "name": "event_type",
        "type": "String"
      },
      {
        "name": "vessel_id",
        "type": "String"
      },
      {
        "name": "vessel_name",
        "type": "String"
      },
      {
        "name": "vessel_flag",
        "type": "String"
      },
      {
        "name": "port_id",
        "type": "String"
      },
      {
        "name": "port_name",
        "type": "String"
      },
      {
        "name": "port_country",
        "type": "String"
      },
      {
        "name": "start_time",
        "type": "DateTime"
      },
      {
        "name": "end_time",
        "type": "DateTime"
      },
      {
        "name": "duration_hours",
        "type": "Decimal"
      }
    ],
    "description": "Processed fishing events data in CSV format"
  }
