      "location": {
        "type": "AzureBlobFSLocation",
        "fileSystem": "processed-data",
        "folderPath": "fishing-events",
        "fileName": "processed_fishing_events.csv"
      },
      "columnDelimiter": ",",
      "rowDelimiter": "\n",
